<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:tools="http://schemas.android.com/tools"
      android:id="@+id/scrollView1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      tools:context="com.myexample.contractdatabase2.SearchActivity" >
      <RelativeLayout
   android:layout_width="match_parent"
   android:layout_height="370dp"
   android:paddingBottom="@dimen/activity_vertical_margin"
   android:paddingLeft="@dimen/activity_horizontal_margin"
   android:paddingRight="@dimen/activity_horizontal_margin"
   android:paddingTop="@dimen/activity_vertical_margin"
   >

	<Button
	    android:id="@+id/button2"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBaseline="@+id/button1"
	    android:layout_alignBottom="@+id/button1"
	    android:layout_centerHorizontal="true"
	    android:text="@string/cancel" />

	<Button
	    android:id="@+id/button1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/TextViewZip"
	    android:layout_below="@+id/TextViewZip"
	    android:layout_marginTop="16dp"
	    android:text="@string/edit" />
<TextView
	    android:id="@+id/editTextName"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_toRightOf="@+id/button1"
	    android:ems="10">

	</TextView>

	<TextView
	    android:id="@+id/TextViewStreet"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignRight="@+id/TextViewName"
	    android:layout_below="@+id/TextViewEmail"
	    android:layout_marginTop="14dp"
	    android:text="@string/street"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/TextViewName"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBaseline="@+id/editTextName"
	    android:layout_alignBottom="@+id/editTextName"
	    android:layout_alignLeft="@+id/TextViewCity"
	    android:text="@string/name"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/EditTextZip"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/button1"
	    android:layout_alignLeft="@+id/EditTextState"
	    android:ems="10" />

	<TextView
	    android:id="@+id/TextViewEmail"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/TextViewStreet"
	    android:layout_below="@+id/TextViewPhone"
	    android:layout_marginTop="16dp"
	    android:text="@string/email"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/editTextPhone"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/EditTextEmail"
	    android:layout_below="@+id/editTextName"
	    android:layout_marginTop="24dp"
	    android:ems="10" />

	<TextView
	    android:id="@+id/TextViewPhone"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBottom="@+id/editTextPhone"
	    android:layout_alignLeft="@+id/TextViewEmail"
	    android:text="@string/phone"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/EditTextEmail"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/TextViewStreet"
	    android:layout_alignLeft="@+id/editTextName"
	    android:ems="10" />

	<TextView
	    android:id="@+id/TextViewCity"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentLeft="true"
	    android:layout_below="@+id/TextViewStreet"
	    android:layout_marginLeft="14dp"
	    android:layout_marginTop="15dp"
	    android:text="@string/city"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/TextViewState"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/TextViewCity"
	    android:layout_below="@+id/TextViewCity"
	    android:layout_marginTop="17dp"
	    android:text="@string/state"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/TextViewZip"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/TextViewStreet"
	    android:layout_below="@+id/TextViewState"
	    android:layout_marginTop="26dp"
	    android:text="@string/zip"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
	    android:id="@+id/EditTextStreet"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/TextViewCity"
	    android:layout_alignLeft="@+id/EditTextEmail"
	    android:ems="10" />

	<TextView
	    android:id="@+id/EditTextCity"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/TextViewState"
	    android:layout_alignLeft="@+id/EditTextStreet"
	    android:ems="10" />

	<TextView
	    android:id="@+id/EditTextState"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/TextViewZip"
	    android:layout_alignLeft="@+id/EditTextCity"
	    android:ems="10" />

</RelativeLayout>
</ScrollView>
